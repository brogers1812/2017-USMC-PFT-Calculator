                ' FEMALE PUSH UP 36-40
                If intAge >= 36 And intAge <= 40 Then
                    intRow = 0
                    strFound = "N"

                    intNumRows = strPushF40Info.GetUpperBound(0) + 1
                    Do Until strFound = "Y" OrElse intRow = intNumRows
                        If strPushF40Info(intRow, 0) = strSearchPush Then
                            strFound = "Y"
                        Else
                            intRow += 1
                        End If
                    Loop

                    If strFound = "Y" Then
                        lblPushScore.Text = strPushF40Info(intRow, 1)
                    Else
                        lblPushScore.Text = "0"
                    End If
                End If
                ' FEMALE PUSH UP 41-45
                If intAge >= 41 And intAge <= 45 Then
                    intRow = 0
                    strFound = "N"

                    intNumRows = strPushF45Info.GetUpperBound(0) + 1
                    Do Until strFound = "Y" OrElse intRow = intNumRows
                        If strPushF45Info(intRow, 0) = strSearchPush Then
                            strFound = "Y"
                        Else
                            intRow += 1
                        End If
                    Loop

                    If strFound = "Y" Then
                        lblPushScore.Text = strPushF45Info(intRow, 1)
                    Else
                        lblPushScore.Text = "0"
                    End If
                End If
                ' FEMALE PUSH UP 46-50
                If intAge >= 46 And intAge <= 50 Then
                    intRow = 0
                    strFound = "N"

                    intNumRows = strPushF50Info.GetUpperBound(0) + 1
                    Do Until strFound = "Y" OrElse intRow = intNumRows
                        If strPushF50Info(intRow, 0) = strSearchPush Then
                            strFound = "Y"
                        Else
                            intRow += 1
                        End If
                    Loop

                    If strFound = "Y" Then
                        lblPushScore.Text = strPushF50Info(intRow, 1)
                    Else
                        lblPushScore.Text = "0"
                    End If
                End If
                ' FEMALE PUSH UP 51+
                If intAge >= 51 Then
                    intRow = 0
                    strFound = "N"

                    intNumRows = strPushF51Info.GetUpperBound(0) + 1
                    Do Until strFound = "Y" OrElse intRow = intNumRows
                        If strPushF51Info(intRow, 0) = strSearchPush Then
                            strFound = "Y"
                        Else
                            intRow += 1
                        End If
                    Loop

                    If strFound = "Y" Then
                        lblPushScore.Text = strPushF51Info(intRow, 1)
                    Else
                        lblPushScore.Text = "0"
                    End If
                End If
                intPush = lblPushScore.Text

                ' Display the Total Score based on indiviual event scores
                lblTotal.Text = intPull + intPush + intCrunch + intRun

            End If
        End If

        ' Displays the appropriate classification based on PFT Score
        If lblTotal.Text >= 235 Then
            lblClass.ForeColor = Color.Green
            lblClass.Text = "First Class"
        Else
            If lblTotal.Text <= 234 And lblTotal.Text >= 200 Then
                lblClass.ForeColor = Color.Orange
                lblClass.Text = "Second Class"
            Else
                If lblTotal.Text <= 199 And lblTotal.Text >= 120 Then
                    lblClass.ForeColor = Color.Red
                    lblClass.Text = "Third Class"
                Else
                    If lblTotal.Text <= 119 Then
                        lblClass.ForeColor = Color.DarkRed
                        lblClass.Text = "FAILED"
                        My.Computer.Audio.Play("ohno.wav")
                    End If
                End If
            End If
        End If
        ' The image zooms out and then back in
        If lblTotal.Text = 300 Then
            Do While pbDog.Height > 0
                pbDog.Height = pbDog.Height - 20
                Me.Refresh()
                System.Threading.Thread.Sleep(100)
            Loop
            Do While pbDog.Height < 120
                pbDog.Height = pbDog.Height + 20
                Me.Refresh()
                System.Threading.Thread.Sleep(100)
            Loop
        Else
            ' The image disappers with a failure
            If lblTotal.Text <= 119 Then
                Do While pbDog.Height > 0
                    pbDog.Height = pbDog.Height - 20
                    Me.Refresh()
                    System.Threading.Thread.Sleep(100)
                Loop
            End If
        End If

    End Sub

    
   
    
